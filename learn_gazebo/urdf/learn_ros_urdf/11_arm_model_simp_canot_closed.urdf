<?xml version="1.0"?>
<robot name="six_arm"> 
<!--creat baselink-->
  <link name="base_link">
    <visual>
      <geometry>
        <box size="0.20 .15 .003"/>
      </geometry>
    </visual>
  </link>

<!--add dody-->
  <link name="body">
    <visual>
      <geometry>
       <box size="0.19 .11 .08"/>
