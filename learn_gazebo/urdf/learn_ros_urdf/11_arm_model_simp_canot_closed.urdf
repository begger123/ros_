  <joint name="base_to_body" type="fixed">
    <parent link="base_link"/>
    <child link="body"/>
    <origin xyz="0.0 0.0 0.04"/>
  </joint> 

<!-- shoulder stevo  XY -->
  <link name="shoulder_stevo">
